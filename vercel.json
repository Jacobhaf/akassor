{
    "version": 2,
    "builds": [
        {
            "src": "blazing-eclipse/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "blazing-eclipse/$1"
        }
    ],
    "rootDirectory": "blazing-eclipse"
}